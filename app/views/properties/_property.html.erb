<div id="<%= dom_id property %>">
  <p>
    <strong>Name:</strong>
    <%= property.name %>
  </p>

  <p>
    <strong>User:</strong>
    <%= property.user_id %>
  </p>

  <p>
    <strong>Address:</strong>
    <%= property.address %>
  </p>

  <p>
    <strong>Latitude:</strong>
    <%= property.latitude %>
  </p>

  <p>
    <strong>Longitude:</strong>
    <%= property.longitude %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= property.price %>
  </p>

  <p>
    <strong>Currency:</strong>
    <%= property.currency %>
  </p>

  <p>
    <strong>Surface area:</strong>
    <%= property.surface_area %>
  </p>

  <p>
    <strong>Bed:</strong>
    <%= property.bed %>
  </p>

  <p>
    <strong>Bath:</strong>
    <%= property.bath %>
  </p>

  <p>
    <strong>Floor:</strong>
    <%= property.floor %>
  </p>

</div>
